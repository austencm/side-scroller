        next.joint = link;  
        physics_joint_set_value(link, phy_joint_damping_ratio, 1);
        physics_joint_set_value(link, phy_joint_frequency, 10);        
        
        with (next)
            parent = other.tail;
    }
        
    // Moving platform
    if (parent != -1) {
        xparentoffset = x - parent.x;
        yparentoffset = y - parent.y;    
    }
    init = true;
}
