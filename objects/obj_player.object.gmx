        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Handle Solid Collisions

// horizontal collision
if (place_meeting(x + hsp, y, obj_wall)) {   
    // auto step-up
    // is this obstruction only one block high? 
    if ( !place_meeting(x + hsp, y - global.BLOCK_HEIGHT, obj_wall) ) {
        y -= global.BLOCK_HEIGHT;
    }
    else {
        // inch towards the obstruction until we're touching it
        while (!place_meeting(x + sign(hsp), y, obj_wall)) {
            x += sign(hsp);
        }
        hsp = 0;
    }
}
x += hsp;

// vertical collision
if ( place_meeting(x, y + vsp, obj_wall) ||
    ( !jumping &amp;&amp; place_meeting(x, y + global.BLOCK_HEIGHT, obj_wall) ) ) {
     
    while (!place_meeting(x, y + sign(vsp), obj_wall)) {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;


</string>
